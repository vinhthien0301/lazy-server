extends layout

block content
    // Wrapper
    #wrapper
        // Miner info
        #minerInfo
            h2= title
            table
                thead
                    th Name
                    th Host
                    th Uptime
                    th ETH/ETC&nbsp;Stats
                    th DCR/SIA&nbsp;Stats
                    if hashrates
                        th ETH/ETC&nbsp;HR
                        th DCR/SIA&nbsp;HR
                    th Temp:Fan
                    th Pools
                    th Ver
                    th Comments
                tbody
        p#minerSummary
        p#lastUpdated
        // /Miner info
    // /Wrapper
